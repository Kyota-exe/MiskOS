diff --git a/sysdeps/tonix/generic/Entry.cpp b/sysdeps/tonix/generic/Entry.cpp
new file mode 100644
index 0000000..57f881f
--- /dev/null
+++ b/sysdeps/tonix/generic/Entry.cpp
@@ -0,0 +1,34 @@
+#include <stdint.h>
+#include <stdlib.h>
+#include <bits/ensure.h>
+#include <mlibc/elf/startup.h>
+
+// defined by the POSIX library
+void __mlibc_initLocale();
+
+extern "C" uintptr_t* __dlapi_entrystack();
+
+extern char** environ;
+static mlibc::exec_stack_data __mlibc_stack_data;
+
+struct LibraryGuard
+{
+    LibraryGuard();
+};
+
+static LibraryGuard guard;
+
+LibraryGuard::LibraryGuard()
+{
+    __mlibc_initLocale();
+
+    // Parse the exec() stack.
+    mlibc::parse_exec_stack(__dlapi_entrystack(), &__mlibc_stack_data);
+    mlibc::set_startup_data(__mlibc_stack_data.argc, __mlibc_stack_data.argv, __mlibc_stack_data.envp);
+}
+
+extern "C" void __mlibc_entry(int (*main_fn)(int argc, char *argv[], char *env[]))
+{
+    auto result = main_fn(__mlibc_stack_data.argc, __mlibc_stack_data.argv, environ);
+    exit(result);
+}
diff --git a/sysdeps/tonix/generic/Generic.cpp b/sysdeps/tonix/generic/Generic.cpp
new file mode 100644
index 0000000..7356feb
--- /dev/null
+++ b/sysdeps/tonix/generic/Generic.cpp
@@ -0,0 +1,255 @@
+#include <bits/ensure.h>
+#include <mlibc/debug.hpp>
+#include <mlibc/all-sysdeps.hpp>
+#include <mlibc/thread-entry.hpp>
+#include <errno.h>
+#include <dirent.h>
+#include <fcntl.h>
+#include <limits.h>
+
+/*
+namespace mlibc
+{
+    int sys_open(const char* path, int flags, int* fd)
+    {
+        int ret;
+        int sys_errno;
+
+        asm volatile("int $0x80" : "=a"(ret) : "D"(0), "S"(path), "d"(flags));
+
+        if (ret == -1)
+            return sys_errno;
+
+        *fd = ret;
+        return 0;
+    }
+}*/
+
+namespace mlibc
+{
+    void sys_libc_log(const char* message)
+    {
+        asm volatile("int $0x80" : : "D"(255), "S"(message));
+    }
+
+    void sys_libc_panic()
+    {
+        asm volatile("int $0x80" : : "D"(254), "S"("mlibc panic!"));
+    }
+
+    int sys_open(const char* path, int flags, int* fd)
+    {
+        int ret;
+        int error;
+        asm volatile("int $0x80" : "=a"(ret), "=b"(error) : "D"(0), "S"(path), "d"(flags));
+
+        if (ret == -1) return error;
+
+        *fd = ret;
+        return 0;
+    }
+
+    int sys_close(int fd)
+    {
+        int ret;
+        int error;
+        asm volatile("int $0x80" : "=a"(ret), "=b"(error) : "D"(4), "S"(fd));
+
+        if (ret == -1) return error;
+
+        return 0;
+    }
+
+    int sys_read(int fd, void* buf, size_t count, ssize_t* bytes_read)
+    {
+        ssize_t ret;
+        int error;
+        asm volatile("int $0x80" : "=a"(ret), "=b"(error) : "D"(1), "S"(fd), "d"(buf), "c"(count));
+
+        if (ret == -1) return error;
+
+        *bytes_read = ret;
+        return 0;
+    }
+
+#ifndef MLIBC_BUILDING_RTDL
+    int sys_write(int fd, const void* buf, size_t count, ssize_t* bytes_written)
+    {
+        ssize_t ret;
+        int error;
+        asm volatile("int $0x80" : "=a"(ret), "=b"(error) : "D"(2), "S"(fd), "d"(buf), "c"(count));
+
+        if (ret == -1) return error;
+
+        *bytes_written = ret;
+        return 0;
+    }
+#endif
+
+    int sys_seek(int fd, off_t offset, int whence, off_t* new_offset)
+    {
+        off_t ret;
+        int error;
+        asm volatile("int $0x80" : "=a"(ret), "=b"(error) : "D"(3), "S"(fd), "d"(offset));
+
+        if (ret == -1) return error;
+
+        *new_offset = ret;
+        return 0;
+    }
+
+    int sys_vm_map(void* hint, size_t size, int prot, int flags, int fd, off_t offset, void** window)
+    {
+        void* ret;
+        int error;
+
+        register off_t* r8 asm("r8") = &offset;
+        asm volatile("int $0x80" : "=a"(ret), "=b"(error) : "D"(5), "S"(hint), "d"(size), "c"(prot), "r"(r8));
+
+        if (ret == nullptr) return error;
+
+        *window = ret;
+        return 0;
+    }
+
+    int sys_tcb_set(void *pointer) {
+        int res;
+        asm volatile ("syscall" : "=a"(res)
+        : "a"(300), "D"(pointer)
+        : "rcx", "r11", "rdx");
+        return res;
+    }
+
+    int sys_anon_allocate(size_t size, void **pointer) {
+        void *ret;
+        int sys_errno;
+
+        asm volatile ("syscall"
+        : "=a"(ret), "=d"(sys_errno)
+        : "a"(9), "D"(0), "S"(size)
+        : "rcx", "r11");
+
+        if (!ret)
+            return sys_errno;
+
+        *pointer = ret;
+        return 0;
+    }
+
+    int sys_anon_free(void *pointer, size_t size) {
+        int unused_return;
+        int sys_errno;
+
+        asm volatile ("syscall"
+        : "=a"(unused_return), "=d"(sys_errno)
+        : "a"(11), "D"(pointer), "S"(size)
+        : "rcx", "r11");
+
+        if (unused_return)
+            return sys_errno;
+
+        return 0;
+    }
+
+#ifndef MLIBC_BUILDING_RTDL
+    void sys_exit(int status) {
+        asm volatile ("syscall" :
+        : "a"(12), "D"(status)
+        : "rcx", "r11", "rdx");
+    }
+#endif
+
+#ifndef MLIBC_BUILDING_RTDL
+    int sys_clock_get(int clock, time_t *secs, long *nanos) {
+        return 0;
+    }
+#endif
+
+    int sys_vm_unmap(void *pointer, size_t size) {
+        return sys_anon_free(pointer, size);
+    }
+
+    int sys_futex_wait(int *pointer, int expected, const struct timespec *time) {
+        uint64_t err;
+        asm volatile ("syscall"
+        : "=d"(err)
+        : "a"(66), "D"(pointer), "S"(expected)
+        : "rcx", "r11");
+
+        if (err) {
+            return -1;
+        }
+
+        return 0;
+    }
+
+    int sys_futex_wake(int *pointer) {
+        uint64_t err;
+        asm volatile ("syscall"
+        : "=d"(err)
+        : "a"(65), "D"(pointer)
+        : "rcx", "r11");
+
+        if (err) {
+            return -1;
+        }
+
+        return 0;
+    }
+
+// All remaining functions are disabled in ldso.
+#ifndef MLIBC_BUILDING_RTDL
+
+    int sys_clone(void *entry, void *user_arg, void *tcb, pid_t *tid_out) {
+        return 0;
+    }
+
+    void sys_thread_exit() {
+        asm volatile ("syscall"
+        :
+        : "a"(68)
+        : "rcx", "r11");
+        __builtin_trap();
+    }
+
+    int sys_sleep(time_t *secs, long *nanos) {
+        return 0;
+    }
+
+    int sys_fork(pid_t *child) {
+        return 0;
+    }
+
+    int sys_execve(const char *path, char *const argv[], char *const envp[]) {
+        int ret;
+        int sys_errno;
+
+        asm volatile ("syscall"
+        : "=a"(ret), "=d"(sys_errno)
+        : "a"(59), "D"(path), "S"(argv), "d"(envp)
+        : "rcx", "r11");
+
+        if (sys_errno != 0)
+            return sys_errno;
+
+        return 0;
+    }
+
+    pid_t sys_getpid() {
+        pid_t pid;
+        asm volatile ("syscall" : "=a"(pid)
+        : "a"(5)
+        : "rcx", "r11", "rdx");
+        return pid;
+    }
+    pid_t sys_getppid() {
+        pid_t ppid;
+        asm volatile ("syscall" : "=a"(ppid)
+        : "a"(14)
+        : "rcx", "r11", "rdx");
+        return ppid;
+    }
+
+#endif // MLIBC_BUILDING_RTDL
+
+} // namespace mlibc
